{
    "name": "sudoku",
    "module": "index.ts",
    "type": "module",
    "dependencies": {
        "dotenv-extended": "^2.9.0",
        "dotenv-parse-variables": "^2.0.0",
        "hono": "^4.5.3",
        "prettier": "^3.3.3"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "@types/dotenv-parse-variables": "^2.0.3"
